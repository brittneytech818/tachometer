{
  "name": "tachometer",
  "version": "0.4.2",
  "description": "Web benchmark runner",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "bin": {
    "tach": "bin/tach.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "rm -rf lib/ client/lib/ && mkdir lib/ && npm run generate-json-schema && tsc && tsc -p client/ && npm run lint",
    "generate-json-schema": "typescript-json-schema tsconfig.json ConfigFile --include src/config.ts --required --noExtraProps > lib/config.schema.json",
    "lint": "tslint --project . --format stylish",
    "format": "find src/ client/src/ -name \"*.ts\" | xargs clang-format --style=file -i",
    "test": "npm run build && mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PolymerLabs/tachometer.git"
  },
  "author": "The Polymer Project Authors",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/PolymerLabs/tachometer/issues"
  },
  "homepage": "https://github.com/PolymerLabs/tachometer#readme",
  "dependencies": {
    "@babel/parser": "^7.4.5",
    "@types/ansi-escape-sequences": "^4.0.0",
    "@types/chai-as-promised": "^7.1.0",
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/fs-extra": "^7.0.0",
    "@types/got": "^9.4.2",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/koa": "^2.0.45",
    "@types/koa-bodyparser": "^4.2.1",
    "@types/koa-mount": "^3.0.1",
    "@types/koa-send": "^4.1.2",
    "@types/koa-static": "^4.0.0",
    "@types/progress": "^2.0.3",
    "@types/selenium-webdriver": "^4.0.0",
    "@types/systeminformation": "^3.23.0",
    "@types/table": "^4.0.5",
    "@types/ua-parser-js": "^0.7.32",
    "ansi-escape-sequences": "^4.0.1",
    "chai-as-promised": "^7.1.1",
    "chromedriver": "^2.45.0",
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.5",
    "fs-extra": "^8.0.1",
    "geckodriver": "^1.14.1",
    "got": "^9.6.0",
    "jsonschema": "^1.2.4",
    "jsonwebtoken": "^8.5.1",
    "jstat": "^1.7.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-mount": "^4.0.0",
    "koa-node-resolve": "^1.0.0-pre.3",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "progress": "^2.0.3",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "source-map-support": "^0.5.6",
    "strip-ansi": "^5.2.0",
    "systeminformation": "^4.1.1",
    "table": "^5.2.1",
    "ua-parser-js": "^0.7.19"
  },
  "devDependencies": {
    "@babel/types": "^7.4.4",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/tmp": "^0.1.0",
    "chai": "^4.2.0",
    "clang-format": "^1.2.4",
    "mocha": "^6.0.2",
    "tmp": "^0.1.0",
    "tslint": "^5.12.1",
    "typescript": "^3.2.0",
    "typescript-json-schema": "^0.38.3"
  }
}
